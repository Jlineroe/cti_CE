        <label for="" class="col-5 col-form-label ">Podría atendernos en los próximos tres días</label>
    </div>
    <div class="col-sm-5">
        <select id="txt_3dias" class="form-control" >
            <option value=""> Seleccione..</option>
            @{
                Dictionary<string, string> puede3dias = new Dictionary<string, string>();
                puede3dias.Add("SI", "SI");
                puede3dias.Add("NO", "NO");
            }
            @foreach (var item in menor)
            {
                if (Model.En_3DIAS != null && Model.En_3DIAS != "" && Model.En_3DIAS.ToUpper() == item.Value.ToUpper())
                {
                    <option value="@item.Value" selected="selected">@item.Value</option>
                }
                else
                {
                    <option value="@item.Value">@item.Value</option>
                }
            }
        </select>
    </div>
