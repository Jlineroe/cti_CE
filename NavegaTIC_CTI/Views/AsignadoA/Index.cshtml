@model IEnumerable<NavegaTIC_CTI.Models.AsignadoA>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Crear nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AsignadoAName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AsignadoAName)
        </td>
        <td>
           @Html.ActionLink("Editar", "Edit", new { id=item.AsignadoAId }) |
           
           @Html.ActionLink("Eliminar", "Delete", new { id=item.AsignadoAId })
        </td>
    </tr>
}

</table>
